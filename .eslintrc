{
  "env": {
      "browser": true,
      "es6": true,
      "node": true
  },
  "extends": [
      "eslint:recommended",
      "airbnb-base",
      "plugin:@typescript-eslint/eslint-recommended",
      "airbnb-typescript/base"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint"
   ],
  "rules": {
    "no-underscore-dangle": "warn"
  }
}
